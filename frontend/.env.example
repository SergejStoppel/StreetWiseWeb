# Frontend Environment Variables
# These are loaded from the main .env file in the root directory

# API URL for frontend to connect to backend
REACT_APP_API_URL=http://localhost:3005

# Supabase URL for direct access to storage (screenshots, etc.)
# This should match DEV_SUPABASE_URL from root .env file
# Example: https://yourproject.supabase.co (no trailing slash)
REACT_APP_SUPABASE_URL=

# Development mode
REACT_APP_DEV_MODE=true

# Enable development tools
REACT_APP_ENABLE_DEV_TOOLS=true