{
  "name": "streetwiseweb",
  "version": "1.0.0",
  "description": "Website Accessibility Analysis Tool",
  "main": "index.js",
  "scripts": {
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "dev": "npm run build:env && concurrently \"npm run backend\" \"npm run frontend\"",
    "dev:production": "APP_ENV=production npm run build:env && concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && npm run dev",
    "frontend": "npm run build:env && cd frontend && npm start",
    "build": "npm run build:env && cd frontend && npm run build",
    "build:dev": "APP_ENV=development npm run build",
    "build:prod": "APP_ENV=production npm run build",
    "build:production": "node scripts/build-production.js",
    "build:env": "node scripts/setup-build-env.js",
    "validate:production": "node scripts/validate-production-env.js",
    "test": "cd backend && npm test && cd ../frontend && npm test",
    "start": "cd backend && npm start",
    "start:dev": "APP_ENV=development npm start",
    "start:prod": "APP_ENV=production npm start",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:dev:detached": "docker-compose -f docker-compose.dev.yml up --build -d",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "docker:prod:detached": "docker-compose -f docker-compose.prod.yml up --build -d",
    "docker:stop": "docker-compose -f docker-compose.dev.yml -f docker-compose.prod.yml down",
    "docker:clean": "docker-compose -f docker-compose.dev.yml -f docker-compose.prod.yml down -v --rmi all",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:logs:backend": "docker-compose -f docker-compose.dev.yml logs -f backend",
    "docker:logs:frontend": "docker-compose -f docker-compose.dev.yml logs -f frontend",
    "docker:simple": "docker-compose -f docker-compose.simple.yml up --build",
    "docker:simple:detached": "docker-compose -f docker-compose.simple.yml up --build -d",
    "docker:simple:stop": "docker-compose -f docker-compose.simple.yml down",
    "health": "node scripts/health-check.js",
    "health:docker": "DOCKER=true node scripts/health-check.js"
  },
  "keywords": [
    "accessibility",
    "web",
    "analysis",
    "audit",
    "compliance"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "dotenv": "^17.2.0"
  }
}
